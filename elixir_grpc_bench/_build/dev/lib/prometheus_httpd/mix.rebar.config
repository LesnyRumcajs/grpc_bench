{erl_opts,[debug_info]}.
{provider_hooks,[{pre,[]},{post,[]}]}.
{post_hooks,[]}.
{deps,[{prometheus,"~> 4.2"},{accept,"0.3.4"}]}.
{shell,[{apps,[prometheus_httpd]}]}.
{cover_enabled,true}.
{cover_export_enabled,true}.
{cover_excl_mods,[]}.
{coveralls_coverdata,"_build/test/cover/ct.coverdata"}.
{coveralls_service_name,"travis-ci"}.
{xref_extra_paths,[]}.
{xref_checks,[undefined_function_calls,undefined_functions,locals_not_used,
              deprecated_function_calls,deprecated_functions]}.
{profiles,
    [{docs,
         [{deps,[{edown,"0.8.1"}]},
          {edoc_opts,
              [{doclet,edown_doclet},
               {dir,"doc"},
               {subpackages,true},
               {overview,"doc/overview.md"},
               {top_level_readme,
                   {"./README.md",
                    "https://github.com/deadtrickster/prometheus_httpd"}}]}]},
     {test,
         [{plugins,[{coveralls,"1.4.0"},{rebar3_elvis_plugin,"0.0.4"}]},
          {erl_opts,[nowarn_export_all]}]}]}.
{plugins,[{rebar3_archive_plugin,"0.0.1"}]}.
{overrides,[]}.
